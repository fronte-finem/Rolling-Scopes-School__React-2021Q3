{
  "name": "react-routing",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "local:dev": "vite --mode localmode",
    "local:build": "tsc && vite build --mode localmode",
    "local:serve": "vite preview --config vite-local-preview.config.ts",
    "generate": "graphql-codegen"
  },
  "dependencies": {
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.0.1",
    "@graphql-codegen/typescript": "^2.0.0",
    "@graphql-codegen/typescript-operations": "^2.0.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/node": "^16.4.13",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "postcss": "^8.3.6",
    "postcss-nested": "^5.0.6",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "vite": "^2.4.4",
    "vite-plugin-imagemin": "^0.4.3",
    "vite-tsconfig-paths": "^3.3.13",
    "vite2-graphql-plugin": "github:araratmartirossyan/vite2-plugin-graphql"
  }
